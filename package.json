{
  "name": "aidpi",
  "version": "1.0.0",
  "description": "Web application to test DPI limitations from Roskomnadzor",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "client-install": "cd client && npm install",
    "server-install": "npm install",
    "install-all": "npm run server-install && npm run client-install",
    "dev-all": "concurrently \"npm run dev\" \"npm run client\""
  },
  "keywords": ["dpi", "roskomnadzor", "censorship", "internet freedom"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "raw-socket": "^1.7.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
}